class User

types
	public Cart = set of (Cart);
	public Wishlist = set of (Wishlist);
	public Email = seq of (char);
	
instance variables
  public cart: Cart;
  public wishlist: Wishlist;
  public email: Email;
  
operations
	public User : Cart * Wishlist * Email ==> User
	User(car,wish,em) == (
	cart := car;
	wishlist := wish;
	email := em;
  return self
	);

end User
